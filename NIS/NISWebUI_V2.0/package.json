{
  "name": "nis-webui",
  "version": "2.0.0",
  "description": "NIS WebUI",
  "main": "index.js",
  "scripts": {
    "dev": "node ./scripts/copy-index.js & concurrently --kill-others -c \"green.bold, yellow.bold\" \"npm run watch\" \"npm run start-dev-server\" ",
    "watch": "webpack --watch --config conf/webpack.dev.conf.js",
    "start-dev-server": "node server/index.js",
    "build": " webpack --config conf/webpack.prod.conf.js & node ./scripts/build-dist.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "config": {
    "dev": {
      "host": "127.0.0.1",
      "port": 3000,
      "publicPath": "http://localhost:3000/"
    },
    "production": {
      "publicPath": "http://localhost/"
    }
  },
  "keywords": [
    "NIS",
    "WebUI"
  ],
  "author": "Meng",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "fs-extra": "^4.0.2",
    "node-sass": "^4.6.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "jquery": "^3.2.1",
    "moment": "^2.19.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
  }
